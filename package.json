{
  "name": "etamitse",
  "version": "0.1.0",
  "description": "Easy and bias-free estimating in meetings.",
  "private": true,
  "license": "MIT",
  "homepage": "https://github.com/ShazzAmin/EtaMitse#readme",
  "bugs": {
    "url": "https://github.com/ShazzAmin/EtaMitse/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ShazzAmin/EtaMitse.git"
  },
  "scripts": {
    "install:backend": "cd backend && npm install",
    "install:frontend": "cd frontend && npm install",
    "postinstall": "run-p --print-label install:*",
    "start:backend": "cd backend && npm start",
    "start:frontend": "cd frontend && npm start",
    "start": "run-p --print-label start:*",
    "build:backend": "cd backend && npm run build",
    "build:frontend": "cd frontend && npm run build",
    "build": "run-p --print-label build:* && rm -rf build/ && mv backend/build/ build/ && mv frontend/build/ build/public/ && cp package.json build/project-package.json"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.3"
  }
}
